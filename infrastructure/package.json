{
  "name": "mindsync-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK Infrastructure for MindSync Core API",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:dev": "cdk deploy MindsyncDevStack --require-approval never",
    "deploy:staging": "cdk deploy MindsyncStagingStack --require-approval never", 
    "deploy:prod": "cdk deploy MindsyncProdStack --require-approval never",
    "destroy:dev": "cdk destroy MindsyncDevStack",
    "bootstrap": "cdk bootstrap",
    "synth": "cdk synth",
    "diff": "cdk diff"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.70.0",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.70.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": ["aws", "cdk", "infrastructure", "serverless", "mindsync"],
  "author": "MindSync Team",
  "license": "MIT"
}
